using ProtoBuf;
using System.Collections.Generic;


/// <summary>
/// 大厅数据
/// </summary>
[ProtoContract(ImplicitFields = ImplicitFields.AllFields)]
public class TownhalData
{
    /// <summary>
    /// 等级
    /// </summary>
    public int level;
    /// <summary>
    /// 已有的物品表
    /// </summary>
    public List<ItemData> existingItemDatas=new List<ItemData>();
    /// <summary>
    /// 可领取的金币
    /// </summary>
    public int availableGold;
    /// <summary>
    /// 当前生命值
    /// </summary>
    public float currentHp;
}

