using ProtoBuf;


/// <summary>
/// 剧本时间数据
/// </summary>
[ProtoContract(ImplicitFields = ImplicitFields.AllFields)]
public class ScriptTimeData
{
    /// <summary>
    /// 秒数
    /// </summary>
    public float second;
    public bool isStartTiming;
    public float lastSecond;
    public float lastDay;
    public float lastMonth;
    public float lastYear;
    public float lastWeek;
}

