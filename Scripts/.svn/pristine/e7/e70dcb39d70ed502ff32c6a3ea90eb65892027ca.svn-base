using UnityEngine;

/// <summary>
/// 
/// </summary>
public partial class NewMainPanel
{
    private GameObject GetBtnObjByType(MainPanelDefine type)
    {
        switch(type)
        {
            case MainPanelDefine.CharBtn:
                return m_charBtn;
            case MainPanelDefine.BagBtn:
                return m_bagBtn;
            case MainPanelDefine.AdvBtn:
                return m_advBtn;
            case MainPanelDefine.CoreBtn:
                return m_coreBtn;
            case MainPanelDefine.HallBtn:
                return m_hallBtn;
            case MainPanelDefine.BarrackBtn:
                return m_barrackBtn;
            case MainPanelDefine.WorkShopBtn:
                return m_workshopBtn;
            case MainPanelDefine.ShopBtn:
                return m_shopBtn;
            case MainPanelDefine.TitleBtn:
                return m_title.gameObject;
            default:
                return null;
        }
    }
}

