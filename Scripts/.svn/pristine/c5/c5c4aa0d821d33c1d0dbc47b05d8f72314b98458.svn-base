using ProtoBuf;


namespace Core.Data
{
    [ProtoContract(ImplicitFields = ImplicitFields.AllFields)]
    public class CoreData
    {
        /// <summary>
        /// 核心等级
        /// </summary>
        public int Level;

        /// <summary>
        /// 核心功率
        /// </summary>
        public float Power;

        /// <summary>
        /// 研究效率
        /// </summary>
        public float Efficiency;

        /// <summary>
        /// 已经累计的魔力值 每日结算 
        /// </summary>
        public float CurrentMana;

        /// <summary>
        /// 是否需要显示对话框
        /// </summary>
        public bool NeedShowDialog;

        /// <summary>
        /// 已经进入过核心
        /// </summary>
        public bool HasIn;
    }

}
